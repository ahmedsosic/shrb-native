{
  "expo": {
    "name": "Saburly",
    "slug": "shrbApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sablogo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#5e71fc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "tothepointcode.shrbApp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/sablogo.png",
        "backgroundColor": "#5e71fc"
      }
    }
  }
}
